{
  "nome": "aprova-facil-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:debug": "node scripts/start-debug.js",
    "test:auth": "node scripts/test-auth-debug.js",
    "test:endpoints": "node scripts/test-endpoints.js",
    "check:debug": "node scripts/check-frontend-debug.js",
    "setup:debug": "node scripts/setup-debug-env.js",
    "test:frontend": "node scripts/test-frontend-data.js",
    "test:auth-fix": "node scripts/test-auth-fix.js",
    "dev:no-turbo": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --format=compact",
    "lint:fix": "next lint --fix",
    "lint:auto-fix": "node scripts/fix-linting.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --config vitest.config.ts --coverage --run",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "test:e2e:watch": "vitest --config vitest.config.e2e.ts",
    "test:e2e:ui": "vitest --ui --config vitest.config.e2e.ts",
    "test:playwright": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:debug": "playwright test --debug",
    "test:visual": "playwright test --config=e2e/visual-tests/playwright.config.ts",
    "test:visual:update": "playwright test --config=e2e/visual-tests/playwright.config.ts --update-snapshots",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:visual": "cypress run --spec \"e2e/cypress/e2e/**/*.cy.ts\"",
    "test:supabase": "node scripts/test-supabase-compatibility.js",
    "fix:hydration": "node scripts/fix-hydration.js",
    "fix:hydration:win": "powershell -ExecutionPolicy Bypass -File scripts/fix-hydration.ps1",
    "clean:cache": "scripts\\clean-cache.bat",
    "test:routes": "scripts\\test-routes.bat",
    "debug:routes": "scripts\\debug-routes.bat",
    "full:reset": "scripts\\full-reset.bat",
    "fix:env": "powershell -ExecutionPolicy Bypass -File scripts/fix-env.ps1",
    "analyze": "npx cross-env ANALYZE=true next build",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:deps": "node scripts/analyze-bundle.js deps",
    "analyze:report": "node scripts/analyze-bundle.js report",
    "analyze:full": "node scripts/analyze-bundle.js full",
    "build:analyze": "npm run build && npm run analyze:bundle",
    "optimize:images": "node scripts/optimize-images.js",
    "type-check": "tsc --noEmit",
    "prepare": "husky install || true"
  },
